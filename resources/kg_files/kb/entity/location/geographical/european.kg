europe=continent()

;

france=country()
in(france,europe)

paris=city()
in(paris,france)

;

germany=country()
in(germany,europe)

berlin=city()
in(berlin,germany)

;

united_kingdom=country()
expr("united kingdom, uk",united_kingdom)
in(united_kingdom,europe)

london=city()
in(london,united_kingdom)

;

italy=country()
in(italy,europe)

rome=city()
in(rome,italy)

;