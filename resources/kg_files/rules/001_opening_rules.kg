imp_trigger(init_ask_name)
-> ask_name_trigger ->
n:<time(b/be(n1/name(), n/name()), now) possess(user, n1)>
request(emora, n)
b{"coldstart": 1}
;

time(be(n1/name(), n/name()), now)
possess(user, n1)
r/request(emora, n)
user_aware(r)
-> welcome_convo ->
h/happy(emora)
time(m/meet(emora, user), now)
cause(m, h)
time(hope(emora, feel(user, good)), now)
p:<time(do(user, p/event(object())), today)>
request(emora, p)
;