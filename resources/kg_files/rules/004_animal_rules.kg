have(user, p/pet())
-> user_own_pet ->
possess(user, p)
;

time(have(user, X/pet()), now)
-> user_has_pet ->
Y:<
    l/like(user, Z/do(user, Y/predicate()))
    with(Z, X)
    time(l, now)
>
user_aware(Z)
request(emora, Y)
;

time(have(user, d/dog()), now)
-> user_has_dog ->

t:<time(know(d, t/trick()), now)>
request(emora, t)

loc:<time(w/walk(user, d), now) locate(w, loc/location())>
user_aware(w)
request(emora, loc)
;