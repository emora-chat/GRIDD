request(emora, m)
;

w/watch(user, m/movie())
specific(m)
user_aware(w)
-> user_watched_specified_movie ->
l/like(user, m)
request_truth(emora, l)
;

/* Next two still a little funky */
h/dislike(user, m/movie())
category(m)
user_aware(h)
-> user_hates_movies ->
p:<c/hate(user, l/movie()) cause(p/predicate(), c) category(l)>
request(emora, p)
;

h/dislike(user, m/movie())
user_aware(h)
-> user_hates_specific_movie ->
p:<c/hate(user, l/movie()) cause(p/predicate(), c) specific(l)>
request(emora, p)
;

