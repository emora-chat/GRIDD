p/buy(X/person(),Y/object())
-> buy_to_ownership ->
possess(X, Y)
;

p/buy(X/person(),Y/house())
time(p,past)
-> buy_house ->
yll/locate(Y, l/location())
time(yll, now)
question(l)
;

buy(X/person(), Y/house())
locate(Y, Z/state())
-> live_state ->
xlz/live(X,Z)
question(like.v.01(X,xlz))
;

buy(X/person(), Y/car())
-> buy_car ->
question(xlc/like.v.01(X, Y))
time(xlc, now)

xdc/drive(X,Y)
xdc{"cover": 1}
question(xld/like.v.01(X, xdc))
time(xld, now)
;
